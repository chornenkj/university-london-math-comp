      "1.9999999998333333\n",
      "1.999999999857143\n",
      "1.999999999875\n",
      "1.9999999998888889\n",
      "1.9999999999\n"
     ]
    }
   ],
   "source": [
    "for i in range(10):\n",
    "    print(get_n_term((i + 1) * 1000000000))"
   ]
